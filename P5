import java.time.Duration;

import org.openqa.selenium.By;

import org.openqa.selenium.WebDriver;

import org.openqa.selenium.WebElement;

import org.openqa.selenium.chrome.ChromeDriver;

public class pgm17 {

 public static void main(String[] args) {

 WebDriver driver = new ChromeDriver();

 try {

 driver.get("https://the-internet.herokuapp.com/upload");

 driver.manage().window().maximize();

 driver.manage().timeouts().implicitlyWait(Duration.ofSeconds(10));

 WebElement fileInput = driver.findElement(By.id("file-upload"));

 fileInput.sendKeys("C:\\Users\\shres\\Desktop\\5th Sem\\DataScience\\DSPSyllabus.pdf");

 WebElement uploadButton = driver.findElement(By.id("file-submit"));

 uploadButton.click();

 WebElement successMessage = driver.findElement(By.id("uploaded-files"));

 if (successMessage.getText().equals("DSPSyllabus.pdf")) {

 System.out.println("File upload successful and verified.");

 } else {
System.out.println("File upload verification failed.");

 }

 } finally {

 driver.quit(); // Ensure proper closing of the browser

 }

 }

}
